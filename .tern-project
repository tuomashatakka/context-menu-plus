{
  "ecmaVersion": "7",
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "src/**"
  ],
  "dontLoad": [
    "node_module/**"
  ],
  "plugins": {
    "doc_comment": true,
    "complete_strings": {
      "maxLength": 15
    },
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "es_modules": {}
  }
}