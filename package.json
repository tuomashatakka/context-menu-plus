{
  "name": "context-menu-plus",
  "main": "./src/index.js",
  "version": "0.0.0",
  "description": "...",
  "keywords": [
    "panel",
    "layout",
    "files",
    "browse",
    "fs",
    "filesystem",
    "explorer",
    "finder",
    "browse",
    "opener",
    "preview"
  ],
  "repository": "https://github.com/tuomashatakka/atom-file-explorer",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0 <2.0.0"
  },
  "styles": [
    "index.less",
    "contextmenu.less"
  ],
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "fs-plus": "^3.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.7.1",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "flow-bin": "^0.54.1",
    "flow-typed": "^2.1.5",
    "flowtype": "^2.0.0"
  },
  "providedServices": {
    "context-menu": {
      "description": "Display a custom view in context menu",
      "versions": {
        "1.0.0": "provideContextMenu"
      }
    }
  },
  "consumedServices": {
    "context-menu": {
      "description": "Display a custom view in context menu",
      "versions": {
        "1.0.0": "consumeContextMenu"
      }
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
      "atom": true,
      "module": true,
      "process": true,
      "require": true,
      "document": true,
      "window": true,
      "__dirname": true,
      "__pathname": true,
      "emit": true,
      "Set": true,
      "Map": true,
      "Proxy": true,
      "Promise": true,
      "WeakSet": true,
      "WeakMap": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "flowtype"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "strict": 0
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "15.5.4"
      },
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    }
  },
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-decorators-legacy"
    ]
  }
