{
  "name": "file-explorer",
  "main": "./src/index.js",
  "version": "0.0.0",
  "description": "...",
  "keywords": [
    "panel",
    "layout",
    "files",
    "browse",
    "fs",
    "filesystem",
    "explorer",
    "finder",
    "browse",
    "opener",
    "preview"
  ],
  "repository": "https://github.com/tuomashatakka/atom-file-explorer",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0 <2.0.0"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "eslint": "^4.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "providedServices": {},
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
      "atom": true,
      "module": true,
      "process": true,
      "require": true,
      "document": true,
      "window": true,
      "__dirname": true,
      "__pathname": true,
      "emit": true,
      "Set": true,
      "Map": true,
      "Proxy": true,
      "Promise": true,
      "WeakSet": true,
      "WeakMap": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "flowtype"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "strict": 0
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "15.5.4"
      },
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    }
  },
  "babel": {
    "presets": ["react"],
    "plugins": [
      "transform-object-rest-spread",
      "transform-decorators-legacy"
    ]
  }
}
